{
  "name": "mystarterkit",
  "version": "1.0.0",
  "description": "JavaScript development environment StarterKit Practice",
  "scripts": {
  "prestart":"node buildScripts/startMessage.js", 
  "open:web" : "node BuildScripts/srcServer.js",  
  "localtunnel": "lt -p 3000 -s saitest123",
  "start":"npm-run-all --parallel open:web lint:watch test:watch",
  "startshare" : "npm-run-all --parallel open:web localtunnel",
  "lint":"esw webpack.config.* src BuildScripts --color",
  "lint:watch":"npm run lint -- --watch",
  
  "babelprestart":"babel-node BuildScripts/startMessage.js",
  "babel-open:web" : "babel-node BuildScripts/srcServer.js",
  "babelStart":"npm-run-all --parallel babelprestart babel-open:web lint:watch test:watch",
  "test":"mocha --reporter progress BuildScripts/testSetup.js \"src/**/*.test.js\"",
  "test-watch":"npm run test -- --watch",

  "clean-dist":"rimraf ./dist && mkdir dist",
  "prebuild": "npm-run-all clean-dist lint test",
  "build":"babel-node BuildScripts/build.js",
  "postbuild":"babel-node BuildScripts/distServer.js"
  },
  "author": "Sai Bhargava",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.3",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "2.1.14",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.14.0",
    "localtunnel": "^1.9.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.0",
    "npm-run-all": "3.1.1",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
